cn: checkmkGuest
olcAttributeTypes: ( 1.3.6.1.4.1.39430.1.1.22 NAME 'cmkGuest' DESC 'A guest user attribute for checkmk.' EQUALITY caseIgnoreMatch SUBSTR caseIgnoreSubstringsMatch SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 SINGLE-VALUE )
olcObjectClasses: ( 1.3.6.1.4.1.39430.1.3.6 NAME 'checkmkGuest' DESC 'checkmkGuest LDAP Schema' AUXILIARY MAY ( cmkGuest ) )

